@{
    ViewData["Title"] = "Display Partners Page";
}

<div class="d-flex">
    <div class="col-md-7">
        <div class="card">

            <div class="card-body">
                <table class="table align-middle mb-0 bg-white">
                    <thead class="bg-light">
                        <tr>
                            <th style="text-align: center;">Hotel</th>
                            <th style="text-align: center;">Ville</th>
                            <th style="text-align: center;">Rating</th>
                            <th style="text-align: center;">Crée en</th>
                            <th style="text-align: center;">Modifié en</th>
                            <th style="text-align: center;">Plus</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td style="text-align: center;">
                                <div class="d-flex align-items-center">
                                    <img src="https://mdbootstrap.com/img/new/avatars/8.jpg" alt=""
                                        style="width: 45px; height: 45px" class="rounded-circle" />
                                    <div class="ms-3">
                                        <p class="fw-bold mb-1">John Doe</p>
                                        <p class="text-muted mb-0">john.doe@gmail.com</p>
                                    </div>
                                </div>
                            </td>

                            <td style="text-align: center;">
                                <span class="badge badge-success rounded-pill d-inline">Active</span>
                            </td>
                            <td style="text-align: center;">Senior</td>
                            <td style="text-align: center;">
                                <button type="button" class="btn btn-link btn-sm btn-rounded">
                                    Edit
                                </button>
                            </td>
                            <td style="text-align: center;">
                                <div class="">
                                    17/02/2022
                                </div>
                            </td>
                            <td class="d-flex justify-content-evenly align-items-center " style="height:2cm ;">
                                <div class="">
                                    <i class="fa-solid fa-eye eye_class"></i>
                                </div>
                            </td>

                        </tr>

                    </tbody>
                </table>
            </div>
            <!-- end card body -->
        </div>
        <!-- end card -->
    </div>
    <div class="col-lg-6">
        <div class="card">
            <div class="card-header justify-content-between d-flex align-items-center">
                <h4 class="card-title">Basic Line Chart</h4>
            </div><!-- end card header -->
            <div class="card-body">
                <div id="line_chart_basic" data-colors='["--bs-primary"]' class="apex-charts" dir="ltr"></div>
            </div>
        </div><!--end card-->
    </div><!--end col-->
    <!-- end col -->
</div>
